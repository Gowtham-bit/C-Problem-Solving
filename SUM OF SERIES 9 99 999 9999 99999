#include <stdio.h>
#include<math.h>
int main()
{
    int a , i ,ne, sum=0 ,square ;
    scanf("%d",&a);
    for(i=1 ; i<=a ; i++)
    {
        square = pow(10,i);
        ne = square - 1;
        sum += ne;
        printf("%d ",ne);
        
    }
    if(a>0)
    {
        printf("\n%d",sum);
    }
    else
    {
        printf("invalid input");
    }
}
